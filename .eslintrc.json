{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    },
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  